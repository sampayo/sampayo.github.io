---
---

[
	{% for post in site.posts {|post| post["categories"][0] == "en" } %}
		{% if post.categories[0] == "en" %}
			{
			  "title"    : "{{ post.title | escape }}",
			  "category" : "{{ post.tags }}",
			  "url"      : "{{ post.url }}",
			  "date"     : "{{ post.date }}"
			} 
				{% if forloop.last  %}

				{% else %}
				,
				{% endif %}

		{% endif %}
	{% endfor %}
]
